      <Property name="PasswordJavaScriptRegEx">^((?=.*\d)(?=.*[a-z])(?=.*[A-Z])).{8,100}$</Property>
      <Property name="RolenameJavaRegEx">^[a-zA-Z0-9-_]{3,30}$</Property>
      <Property name="RolenameJavaScriptRegEx">^[a-zA-Z0-9-_]{3,30}$</Property>
      <Property name="MultiAttributeSeparator">,</Property>
